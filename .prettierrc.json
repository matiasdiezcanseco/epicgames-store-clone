{
    "trailingComma": "es5",
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100,
    "endOfLine": "lf",
    "plugins": ["@ianvs/prettier-plugin-sort-imports"],
    "importOrder": [
        "^react$",
        "<BUILTIN_MODULES>",
        "<THIRD_PARTY_MODULES>",
        "",
        "^[.]",
        "",
        "<TYPES>",
        "<TYPES>^[.]",
        "",
        "^(?!.*[.]css$)[./].*$",
        ".css$"
    ],
    "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
    "importOrderTypeScriptVersion": "5.0.0"
}
